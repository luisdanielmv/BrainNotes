{
  "name": "brainnotes",
  "version": "1.0.0",
  "description": "Evernotes Clone",
  "main": "index.js",
  "scripts": {
    "server": "http-server ./dist",
    "presass": "cp ./src/index.html ./dist/ && cp -R node_modules/mdi/fonts ./dist/fonts",
    "sass": "node-sass ./src/scss/main.scss --include-path ./src/scss/ --include-path ./node_modules/bootstrap-sass/assets/stylesheets/ --include-path ./node_modules/mdi/scss/ -o ./dist/css/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisdanielmv/BrainNotes.git"
  },
  "author": "Daniel Morales, Melvin Salas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luisdanielmv/BrainNotes/issues"
  },
  "homepage": "https://github.com/luisdanielmv/BrainNotes#readme",
  "devDependencies": {
    "http-server": "^0.10.0",
    "node-sass": "^4.5.2"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "mdi": "^1.9.33"
  }
}
